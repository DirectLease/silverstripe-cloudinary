{
  "name": "level51/silverstripe-cloudinary",
  "version": "0.2.1",
  "time": "2019-03-19",
  "description": "Cloudinary image handling & upload field for SilverStripe",
  "keywords": [
    "silverstripe",
    "cloudinary"
  ],
  "type": "silverstripe-vendormodule",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Kliemsch",
      "email": "dk@lvl51.de"
    }
  ],
  "require": {
    "php": ">=7.0",
    "silverstripe/framework": "^4.3",
    "cloudinary/cloudinary_php": "^1.9",
    "nesbot/carbon": "^1.34"
  },
  "extra": {
    "installer-name": "cloudinary",
    "expose": [
      "client"
    ]
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Lvl51\\Cloudinary\\": "src/"
    }
  }
}
